<!DOCTYPE html>
<html>
  <head>
    <title>测试首页</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>首页</h1>
    <h3 class="userInfo"></h3>
    <p>Welcome to IndexPage</p>
  </body>
  <script src="/javascripts/jquery-3.1.0.min.js"></script>
  <script>
      $(function() {
          $.ajax({
            url:'/getUserInfo',
            type:'post',
            success:function(res){
              console.log(res);
              $('.userInfo').text('欢迎你,'+res.returnData.username+'!');
            }
          })
      })
  </script>
</html>
